<?xml version="1.0" encoding="UTF-8"?>
<!--

    This test file contains an duplicate edge id.
    This is not allowed, but cannot be detected by the XSD-validator.
    We expect the parser to detect this.

-->
<templates>
    <template name="Template" superpattern="Super">
        <structure>
            <nodes>
                <node id="A" type="CLASS"/>
                <node id="B" type="CLASS"/>
            </nodes>
            <edges>
                <edge id="AB" node1="A" node2="B"/>
                <!-- A duplicate edge id... -->
                <edge id="AB" node1="B" node2="A"/>
            </edges>
        </structure>
        <conditions>
            <condition id="1" description="A has a directed association with B.">
                <rules>
                    <rule applies="AB"
                          scope="RELATION"
                          topic="TYPE"
                          operation="EQUALS"
                          value="ASSOCIATION_DIRECTED"/>
                </rules>
            </condition>
            <condition id="2" description="B has a directed association with A.">
                <rules>
                    <rule applies="BA"
                          scope="RELATION"
                          topic="TYPE"
                          operation="EQUALS"
                          value="ASSOCIATION_DIRECTED"/>
                </rules>
            </condition>
        </conditions>
    </template>
</templates>
    
